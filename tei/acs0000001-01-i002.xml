                            <pos>n</pos>
                            <usg>archaic</usg>
                            <usg>poetic/literary</usg>
                        </gramGrp>
                        <sense>
                            <def>outrage; injury</def>
                        </sense>
                        <sense>
                            <def>contempt; disdain</def>
                        </sense>
                    </hom>
                </entry><entry xml:id="confluent">
                    <form>
                        <orth>confluent</orth>
                    </form>
                    <def>Flowing together or merging.</def>
                </entry><entry xml:id="culminant">
